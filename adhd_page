<head>
  <script>
    // This script will make all <h6> tags behave like <h1>
    window.onload = function() {
        document.querySelectorAll('h6').forEach(h6 => {
            h6.style.fontSize = '2em'; // Adjust the size like an <h1>
            h6.style.fontWeight = 'bold'; // Make it bold like an <h1>
            h6.style.margin = '0.67em 0'; // Adjust margin like an <h1>
            h6.style.lineHeight = '1.2'; // Adjust line height for better spacing
        });
    };
</script>
</head>

<script>
  function toggleGambot() {
    const box = document.getElementById("gambotMoments");
    box.style.display = box.style.display === "none" ? "block" : "none";
  }
  
  let gambotClicks = 0;
  function incrementGambot() {
    gambotClicks++;
    document.getElementById('gambotClickCount').textContent = `Clicks: ${gambotClicks}`;
  }
  </script>
  
  <div id="gambotClicker" style="
