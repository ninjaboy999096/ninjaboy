<script>
  // JavaScript to make h7-h50 behave like normal headings
  document.querySelectorAll('h7, h8, h9, h10, h11, h12, h13, h14, h15, h16, h17, h18, h19, h20, h21, h22, h23, h24, h25, h26, h27, h28, h29, h30, h31, h32, h33, h34, h35, h36, h37, h38, h39, h40, h41, h42, h43, h44, h45, h46, h47, h48, h49, h50')
    .forEach(function(element) {
      // Create an h6 element to mimic h7 and beyond
      let level = parseInt(element.tagName.substring(1)); // Extracts the number (e.g., h7 => 7)
      let heading = document.createElement('h' + Math.min(level, 6)); // Limit to max h6
      heading.innerHTML = element.innerHTML; // Transfer content
      element.parentNode.replaceChild(heading, element); // Replace with new heading
    });
</script>
 <!-- Note: ChatGPT helped (or made) the part above -->
